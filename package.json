{
  "name": "spyglass",
  "version": "1.0.0",
  "description": "A Contentstack + Valtech demo",
  "main": "src/index.js",
  "browserslist": [
    "since 2019-08"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build ./src/scenario2/index.html --target browser --no-minify --out-dir ./docs/skincare --public-url ./",
    "dev": "parcel ./src/index.html --no-hmr --target browser --https --out-dir ./dev",
    "dev:scenario2": "parcel ./src/scenario2/scenario2.html --no-hmr --target browser --https --out-dir ./scenario2/dev",
    "dev:contentstack": "parcel ./samples/contentstack-integration/index.html --no-hmr --target browser --out-dir ./samples/contentstack-integration/dev",
    "dev:rotation": "parcel ./samples/rotation/rotation-marker.html --https --no-hmr --target browser --out-dir ./samples/rotation/dev",
    "dev:overlay": "parcel ./samples/camera-overlay/camera-overlay.html --https --no-hmr --target browser --out-dir ./samples/camera-overlay/dev",
    "dev:multirotation": "parcel ./samples/multi-marker-twist/multi-marker-twist.html --https --no-hmr --target browser --out-dir ./samples/multi-marker-twist/dev",
    "dev:s1html": "parcel ./samples/scenario-1-template/index.html --https --no-hmr --target browser --out-dir ./samples/scenario-1-template/dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valtech-sd/spyglass.git"
  },
  "author": "Valtech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valtech-sd/spyglass/issues"
  },
  "homepage": "https://github.com/valtech-sd/spyglass#readme",
  "dependencies": {},
  "devDependencies": {
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.4"
  }
}
